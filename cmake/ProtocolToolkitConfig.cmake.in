@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/ProtocolToolkitTargets.cmake")

# Define the library target
if(NOT TARGET ProtocolToolkit::protocol_toolkit)
    message(FATAL_ERROR "ProtocolToolkit::protocol_toolkit target not found")
endif()

# Set variables for backward compatibility
set(ProtocolToolkit_LIBRARIES ProtocolToolkit::protocol_toolkit)
set(ProtocolToolkit_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")

check_required_components(ProtocolToolkit)